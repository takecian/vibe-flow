{
  "name": "vibe-flow",
  "version": "1.0.0",
  "description": "Local task management with Git worktrees and AI integration",
  "main": "bin/vibe-flow.js",
  "bin": {
    "vibe-flow": "./bin/vibe-flow.js"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w server\" \"npm run dev -w client\"",
    "build": "npm run build -w client && npm run build -w server",
    "start": "node bin/vibe-flow.js"
  },
  "keywords": [
    "git",
    "kanban",
    "ai",
    "workflow"
  ],
  "author": "USER",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "open": "^10.1.0",
    "prompts": "^2.4.2",
    "yargs": "^17.7.2",
    "dotenv": "^16.4.5",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "lowdb": "^7.0.1",
    "node-pty": "0.10.0",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "typescript": "^5.3.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.11.20",
    "@types/lowdb": "^1.0.15",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^9.0.8"
  }
}